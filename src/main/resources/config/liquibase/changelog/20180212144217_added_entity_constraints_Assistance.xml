<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Assistance.
    -->
    <changeSet id="20180212144217-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="shop_id"
                                 baseTableName="assistance"
                                 constraintName="fk_assistance_shop_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shop"/>

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="assistance"
                                 constraintName="fk_assistance_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

    </changeSet>
</databaseChangeLog>
